CREATE OR REPLACE PROCEDURE add_category (id IN VARCHAR2, name IN VARCHAR2, "desc" IN VARCHAR2)
IS
  id_len NUMBER;
BEGIN
  id_len := LENGTH(id);
  
  IF id_len < 6 THEN
    RAISE_APPLICATION_ERROR(-20001, 'Category_id must be at least 6 characters long');
  ELSE
    INSERT INTO Product_category (Category_id, Name, Description)
    VALUES (id, name, desc);
  END IF;
END;
